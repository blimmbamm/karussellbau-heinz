{
  "name": "cms",
  "private": true,
  "version": "1.0.0",
  "main": "package.json",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "deploy-graphql": "sanity graphql deploy",
    "typegen": "sanity schema extract && sanity typegen generate",
    "translation:test": "tsx --env-file=.env ./translation/testPageTranslation.ts",
    "seo-description:test": "tsx --env-file=.env ./seo/testSeoDescriptionGeneration.ts",
