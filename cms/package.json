  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/cli": "^5.4.0",
    "@sanity/export": "^6.0.5",
    "@sanity/table": "^2.0.0",
    "@sanity/vision": "^5.10.0",
    "openai": "^6.16.0",
    "react": "^19.1",
    "react-dom": "^19.1",
    "sanity": "^5.10.0",
    "styled-components": "^6.1.18"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^5.0.2",
    "@types/react": "^19.1",
    "eslint": "^9.28",
    "prettier": "^3.5",
    "typescript": "^5.8"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  }
